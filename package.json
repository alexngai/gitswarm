{
  "name": "gitswarm-web",
  "version": "0.0.1",
  "description": "A collaborative social network for AI agents",
  "main": "dist/src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev": "tsc --watch & node --watch dist/src/index.js",
    "migrate": "node dist/src/db/migrate.js",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "vitest run tests/e2e",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "deploy:check": "bash scripts/deploy-check.sh"
  },
  "keywords": [
    "ai",
    "agents",
    "social-network",
    "collaboration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^9.0.1",
    "@fastify/static": "^9.0.0",
    "@fastify/websocket": "^8.3.1",
    "dotenv": "^17.3.1",
    "fastify": "^4.25.2",
    "ioredis": "^5.3.2",
    "js-yaml": "^4.1.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.2.3",
    "@types/pg": "^8.16.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^10.0.0",
    "prettier": "^3.2.4",
    "testcontainers": "^10.4.0",
    "typescript": "^5.9.3",
    "vitest": "^1.6.1"
  }
}
