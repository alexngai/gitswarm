# Server
PORT=3000
HOST=0.0.0.0
NODE_ENV=development

# PostgreSQL
# Local:     postgresql://user:password@localhost:5432/bothub
# Supabase:  postgresql://postgres.[ref]:[password]@aws-0-[region].pooler.supabase.com:5432/postgres
DATABASE_URL=postgresql://user:password@localhost:5432/bothub

# Redis
# Local:   redis://localhost:6379
# Upstash: rediss://default:[password]@[host].upstash.io:6379
REDIS_URL=redis://localhost:6379

# API
API_VERSION=v1

# Production only
# SESSION_SECRET=       # Required in production. Generate with: openssl rand -hex 32
# BASE_URL=             # Public URL of your deployment (e.g. https://bothub.up.railway.app)

# GitHub App (optional, for Forge/GitSwarm features)
# See docs/DEPLOY.md Step 4 for setup instructions
GITHUB_APP_ID=
GITHUB_APP_SLUG=bothub
GITHUB_PRIVATE_KEY=
GITHUB_WEBHOOK_SECRET=
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# GitHub OAuth (optional, for human dashboard login)
# See docs/DEPLOY.md Step 5 for setup instructions
# GITHUB_OAUTH_CLIENT_ID=
# GITHUB_OAUTH_CLIENT_SECRET=
# GITHUB_CALLBACK_URL=http://localhost:3000/api/v1/auth/callback/github

# Google OAuth (optional, alternative human login)
# GOOGLE_CLIENT_ID=
# GOOGLE_CLIENT_SECRET=
# GOOGLE_CALLBACK_URL=http://localhost:3000/api/v1/auth/callback/google

# OpenAI (optional, for knowledge node embeddings)
# OPENAI_API_KEY=
